---
import WhatsAppWidget from "@/components/WhatsAppWidget.astro";
import Footer from "@/components/Footer.astro";

const { disableFooter = false } = Astro.props as {
  disableFooter: boolean;
};
---

<!doctype html>
<html lang="en">
  <!-- Mirrored from ultimatewebsolutions.net/mireya/light/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 13 Jul 2025 15:52:57 GMT -->
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <!-- color of address bar in mobile browser -->
    <meta name="theme-color" content="#28292C" />
    <!-- favicon  -->
    <link
      rel="shortcut icon"
      href="/mireya/img/light/favicon.png"
      type="image/x-icon"
    />

    <!-- Minterirors -->

    <link
      rel="stylesheet"
      type="text/css"
      href="/minterior/assets/css/animate.css"
    />

    <link
      rel="stylesheet"
      type="text/css"
      href="/minterior/assets/css/all.min.css"
    />

    <link
      rel="stylesheet"
      type="text/css"
      href="/minterior/assets/css/line-awesome.min.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/minterior/assets/css/slick.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/minterior/assets/css/slick-theme.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/minterior/assets/css/color.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/minterior/assets/css/style.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/minterior/assets/css/responsive.css"
    />

    <!-- bootstrap css -->
    <link rel="stylesheet" href="/mireya/css/plugins/bootstrap.min.css" />
    <!-- font awesome css -->
    <link rel="stylesheet" href="/mireya/css/plugins/font-awesome.min.css" />
    <!-- swiper css -->
    <link rel="stylesheet" href="/mireya/css/plugins/swiper.min.css" />
    <!-- fancybox css -->
    <link rel="stylesheet" href="/mireya/css/plugins/fancybox.min.css" />
    <!-- mapbox css -->
    <link href="/mireya/css/plugins/mapbox-style.css" rel="stylesheet" />
    <!-- main css -->
    <link rel="stylesheet" href="/mireya/css/style-light.css" />

    <title>Mireya</title>
  </head>

  <body>
    <div class="mry-app">
      <!-- preloader -->
      <div class="mry-preloader mry-active">
        <div class="mry-preloader-content">
          <img
            class="mry-logo mry-mb-20"
            src="/mireya/img/light/logo.svg"
            alt="Mireya"
          />
          <div class="mry-loader-bar">
            <div class="mry-loader"></div>
          </div>
          <div class="mry-label">Please wait</div>
        </div>
      </div>
      <!-- preloader end -->

      <!-- cursor -->
      <div class="mry-magic-cursor">
        <div class="mry-ball">
          <div class="mry-loader">
            <svg
              version="1.1"
              xmlns="http://www.w3.org/2000/svg"
              xmlns:xlink="http://www.w3.org/1999/xlink"
              x="0px"
              y="0px"
              width="40px"
              height="40px"
              viewBox="0 0 50 50"
              style="enable-background:new 0 0 50 50;"
              xml:space="preserve"
            >
              <path
                d="M25.251,6.461c-10.318,0-18.683,8.365-18.683,18.683h4.068c0-8.071,6.543-14.615,14.615-14.615V6.461z"
              >
                <animateTransform
                  attributeType="xml"
                  attributeName="transform"
                  type="rotate"
                  from="0 25 25"
                  to="360 25 25"
                  dur="0.6s"
                  repeatCount="indefinite"></animateTransform>
              </path>
            </svg>
          </div>
        </div>
      </div>
      <!-- cursor end -->

      <!-- top panel -->
      <div class="mry-top-panel" style="background-color: #f7f8fa;">
        <div class="mry-logo-frame">
          <a href="index.html" class="mry-default-link mry-anima-link">
            <img
              class="mry-logo"
              src="/mireya/img/light/logo.svg"
              alt="Mireya"
            />
          </a>
        </div>
        <div class="mry-menu-button-frame">
          <div class="mry-label">Menu</div>

          <div class="mry-menu-btn mry-magnetic-link">
            <div class="mry-burger mry-magnetic-object">
              <span></span>
            </div>
          </div>
        </div>
      </div>
      <!-- top panel end -->

      <!-- menu -->
      <div class="mry-menu">
        <div class="container">
          <div class="row justify-content-between">
            <div class="col-md-4">
              <nav
                id="mry-dynamic-menu"
                style="width:fit-content; padding-left:0 !important;"
              >
                <ul>
                  <li class="menu-item">
                    <a href="/" class="mry-anima-link mry-default-link">Home</a>
                  </li>
                  <li class="menu-item">
                    <a href="/about" class="mry-anima-link mry-default-link"
                      >About Us</a
                    >
                  </li>
                  <li class="menu-item">
                    <a href="/services" class="mry-anima-link mry-default-link"
                      >Services</a
                    >
                  </li>
                  <li class="menu-item">
                    <a href="/our-work" class="mry-anima-link mry-default-link"
                      >Our Work</a
                    >
                  </li>
                  <li class="menu-item">
                    <a href="/contact" class="mry-anima-link mry-default-link"
                      >Contact</a
                    >
                  </li>
                </ul>
              </nav>
            </div>
            <div class="col-md-4">
              <div class="mry-info-box-frame">
                <div class="mry-info-box">
                  <div class="mry-mb-20">
                    <div class="mry-label mry-mb-5">Country:</div>
                    <div class="mry-text">Canada</div>
                  </div>
                  <div class="mry-mb-20">
                    <div class="mry-label mry-mb-5">City:</div>
                    <div class="mry-text">Toronto</div>
                  </div>
                  <div class="mry-mb-20">
                    <div class="mry-label mry-mb-5">Adress:</div>
                    <div class="mry-text">HTGS 4456 North Av.</div>
                  </div>
                  <div class="mry-mb-20">
                    <div class="mry-label mry-mb-5">Email:</div>
                    <a class="mry-text" href="mailto:mireya.inbox@mail.com"
                      >mireya.inbox@mail.com</a
                    >
                  </div>
                  <div class="mry-mb-20">
                    <div class="mry-label mry-mb-5">Phone:</div>
                    <a class="mry-text" href="#.">+4 9(054) 996 84 25</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- menu end -->

      <div class="transition-fade">
        <slot />

        {!disableFooter && <Footer />}
      </div>
    </div>

    <script defer src="/minteriors/assets/js/popper.js"></script>
    <script defer src="/minteriors/assets/js/bootstrap.min.js"></script>
    <script defer src="/minteriors/assets/js/plugin/slick.min.js"></script>
    <script defer src="/minteriors/assets/js/plugin/html5lightbox.js"></script>
    <script defer src="/minteriors/assets/js/placeholdem.min.js"></script>
    <script defer src="/minteriors/assets/js/wow.min.js"></script>
    <script defer src="/minteriors/assets/js/script.js"></script>

    <!-- jquery js -->
    <script defer src="/mireya/js/plugins/jquery.min.js"></script>
    <!-- tween max js -->
    <script defer src="/mireya/js/plugins/tween-max.min.js"></script>
    <!-- scroll magic js -->
    <script defer src="/mireya/js/plugins/scroll-magic.js"></script>
    <!-- scroll magic gsap plugin js -->
    <script defer src="/mireya/js/plugins/scroll-magic-gsap-plugin.js"></script>
    <!-- swiper js -->
    <script defer src="/mireya/js/plugins/swiper.min.js"></script>
    <!-- isotope js -->
    <script defer src="/mireya/js/plugins/isotope.min.js"></script>
    <!-- fancybox js -->
    <script defer src="/mireya/js/plugins/fancybox.min.js"></script>
    <!-- mapbox js -->
    <script defer src="/mireya/js/plugins/mapbox.min.js"></script>
    <!-- smooth scrollbar js -->
    <script defer src="/mireya/js/plugins/smooth-scrollbar.min.js"></script>
    <!-- overscroll js -->
    <script defer src="/mireya/js/plugins/overscroll.min.js"></script>
    <!-- canvas js -->
    <script defer src="/mireya/js/plugins/canvas.js"></script>
    <!-- parsley js -->
    <script defer src="/mireya/js/plugins/parsley.min.js"></script>
    <!-- main js -->
    <script defer src="/mireya/js/main.js"></script>

    <WhatsAppWidget />
  </body>

  <!-- Mirrored from ultimatewebsolutions.net/mireya/light/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 13 Jul 2025 15:53:18 GMT -->
</html>
