---
import MainLayout from "../../lib/layouts/main-layout.astro";
---

<MainLayout>
  <!-- project -->
  <div class="mry-about mry-p-100-100">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-8">
          <div class="mry-mb-100 mry-text-center">
            <div class="mry-numbering mry-fo">
              <div class="mry-border-text">01</div>
              <div class="mry-subtitle">Goal</div>
            </div>
            <h3 class="mry-mb-40 mry-fo">
              Jane's House<br />
              <span class="mry-border-text"
                >Modern Living with a Timeless Touch
              </span>
            </h3>
            <p class="mry-text mry-fo">
              This contemporary minimalist home embraces simplicity with a
              luxurious edge. Thoughtful design, clean lines, and high-end
              finishes come together in a space that feels open, elegant, and
              effortlesslyÂ livable.
            </p>
          </div>
        </div>
        <div class="col-lg-12">
          <div class="mry-main-title mry-title-center mry-p-0-40">
            <div class="mry-numbering mry-fo">
              <div class="mry-border-text">02</div>
              <div class="mry-subtitle">Project</div>
            </div>
            <h2 class="mry-fo">Project gallery</h2>
          </div>
        </div>
        <div class="col-lg-12">
          <div class="mry-portfolio-frame">
            <div class="mry-masonry-grid mry-without-descr mry-p-0-100">
              <div class="mry-grid-sizer"></div>

              <div
                class="mry-masonry-grid-item mry-masonry-grid-item-50 interior"
              >
                <div class="mry-card-item">
                  <div class="mry-card-cover-frame mry-mb-20 mry-fo">
                    <img
                      src="https://res.cloudinary.com/daoju0r3c/image/upload/v1755030744/janed_zg2h1m.jpg"
                      alt="project"
                    />
                    <div class="mry-cover-overlay"></div>
                    <div class="mry-hover-links">
                      <a
                        href="https://res.cloudinary.com/daoju0r3c/image/upload/v1755030744/janed_zg2h1m.jpg"
                        data-fancybox="works"
                        class="mry-zoom mry-magnetic-link"
                        ><span class="mry-magnetic-object"
                          ><i class="fas fa-expand"></i></span
                        ></a
                      >
                    </div>
                  </div>
                </div>
              </div>
              <div
                class="mry-masonry-grid-item mry-masonry-grid-item-50 mry-masonry-grid-item-h-x-2 repair"
              >
                <div class="mry-card-item">
                  <div class="mry-card-cover-frame mry-mb-20 mry-fo">
                    <div class="mry-cover-overlay"></div>
                    <img
                      src="https://res.cloudinary.com/daoju0r3c/image/upload/v1755030706/janec_diissg.jpg"
                      alt="project"
                    />
                    <div class="mry-hover-links">
                      <a
                        href="https://res.cloudinary.com/daoju0r3c/image/upload/v1755030706/janec_diissg.jpg"
                        data-fancybox="works"
                        class="mry-zoom mry-magnetic-link"
                        ><span class="mry-magnetic-object"
                          ><i class="fas fa-expand"></i></span
                        ></a
                      >
                    </div>
                  </div>
                </div>
              </div>
              <div
                class="mry-masonry-grid-item mry-masonry-grid-item-50 interior repair"
              >
                <div class="mry-card-item">
                  <div class="mry-card-cover-frame mry-mb-20 mry-fo">
                    <img
                      src="https://res.cloudinary.com/daoju0r3c/image/upload/v1755030745/janee_uwhvmy.jpg"
                      alt="project"
                    />
                    <div class="mry-cover-overlay"></div>
                    <div class="mry-hover-links">
                      <a
                        href="https://res.cloudinary.com/daoju0r3c/image/upload/v1755030745/janee_uwhvmy.jpg"
                        data-fancybox="works"
                        class="mry-zoom mry-magnetic-link"
                        ><span class="mry-magnetic-object"
                          ><i class="fas fa-expand"></i></span
                        ></a
                      >
                    </div>
                  </div>
                </div>
              </div>

              <div
                class="mry-masonry-grid-item mry-masonry-grid-item-50 mry-masonry-grid-item-h-x-1 repair"
              >
                <div class="mry-card-item">
                  <div class="mry-card-cover-frame mry-mb-20 mry-fo">
                    <div class="mry-cover-overlay"></div>
                    <img
                      src="https://res.cloudinary.com/daoju0r3c/image/upload/v1755030762/janea_wlwsq5.jpg"
                      alt="project"
                    />
                    <div class="mry-hover-links">
                      <a
