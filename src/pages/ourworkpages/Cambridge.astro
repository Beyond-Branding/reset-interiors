<style>

  .mry-banner {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    min-height: 100vh;
    overflow: hidden;
    padding: 0 !important; 
  }

  .mry-banner .container {
    position: relative; 
    z-index: 2;
  }

  .mry-head-bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1; 
  }

  .mry-head-bg img,
  .mry-head-bg .mry-bg-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .mry-head-bg img {
    opacity: 100%; 
  }
</style>